<div class="container">
  <div class="row">
    <ol class="breadcrumb">
      <li>
        <a href="/">Home</a>
      </li>
      <li>
        <a href="/admin">Admin Menu</a>
      </li>
      <li class="active">Current</li>
    </ol>

<h1>Listing tickets</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>User</th>
      <th>Umbrella</th>
      <th>Gps present lat</th>
      <th>Gps present lon</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tickets.each do |ticket| %>
      <tr>
        <td><%= ticket.user %></td>
        <td><%= ticket.umbrella %></td>
        <td><%= ticket.gps_present_lat %></td>
        <td><%= ticket.gps_present_lon %></td>
        <td><%= link_to 'Show', ticket %></td>
        <td><%= link_to 'Edit', edit_ticket_path(ticket) %></td>
        <td><%= link_to 'Destroy', ticket, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ticket', new_ticket_path %>

</div>
</div>